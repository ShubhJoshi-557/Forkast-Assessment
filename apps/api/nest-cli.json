{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "watchAssets": false
  },
  "watchOptions": {
    "watchAssets": false,
    "ignore": [
      "node_modules/**/*",
      "dist/**/*",
      "**/*.log",
      "**/*.tmp",
      "**/.prisma/**/*",
      "**/.cache/**/*",
      "**/coverage/**/*"
    ]
  }
}
